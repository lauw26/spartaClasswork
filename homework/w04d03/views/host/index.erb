<!-- yield in layout.rb is where the elements below is placed  -->
<!-- linking html element navigation bar from navigation to veiw using erb -->

<% @songs.each do |song| %>
	<div class="row">
  		<div class="col-md-2"></div>
	  		<div class="col-md-8">
	  			<h3><a href="/<%= song.id %>"><%= song.id + 1 %>. <%= song.name %> <%= song.rating %>/5</a></h3>
	  		</div>
  		<div class="col-md-2"></div>
	</div>
<% end %>